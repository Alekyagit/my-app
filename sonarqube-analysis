node{
   stage('SCM Checkout'){
     git 'https://github.com/Alekyagit/my-app'
   }
   stage('Compile-Package'){
      // Get maven home path
      def mvn =  tool name: 'maven', type: 'maven'   
      sh "${mvn}/bin/mvn package"
   }
   
   stage('SonarQube Analysis') {
        def mvn =  tool name: 'maven', type: 'maven'
        withSonarQubeEnv('sonar-6') { 
          sh "${mvn}/bin/mvn sonar:sonar"
        }
    }
}
